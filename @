#include<iostream>

#include<vector>
#include<math.h>
#include<string>
#include<algorithm>
#include<iomanip>
using namespace std;

int main() {
	int begin, n;
	cin >> begin >> n;
	int sum = 0;
	if (begin < 0) {
		while (n) {
			int k = 0;
			cout << 0 + k << endl;
			k += 2;
			n--;
		}
	} else {
		while (n) {
			int k;
			if (begin % 2 == 0) k = 0;
			else k = 1;
			cout << begin + k << endl;
			k += 2;
			n--;
		}
	}
}

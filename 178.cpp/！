#include<iostream>
#include<string>
using namespace std;
int main() {
	int n;
	cin >> n;
	string str;
	string st;
	str = "A";
	int i = 1;
	while(i < n){
		st = str.substr(0);
		str = str + 'A + i - 1';
		str.insert(str.length() - 1, st);
		i++;
	}
	cout << str;
}
